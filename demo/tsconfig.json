{
	"compilerOptions": {
		"target": "es2020",
		"module": "esnext",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"sourceMap": true,
		"declaration": true,
		"jsx": "react",
		"strict": true,
		"outDir": "./dist",
		"esModuleInterop": true
	}
}
