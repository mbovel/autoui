        "@types/uuid": "^3.4.6",
        "automerge": "^0.12.1",
        "classnames": "^2.2.6",
        "lodash-es": "^4.17.15",
        "preact": "^10.1.1",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "redux": "^4.0.4",
        "uuid": "^3.3.3"
    },
    "devDependencies": {
        "copyfiles": "^2.1.1",
        "csstype": "^2.6.8",
        "npm-check-updates": "^4.0.1",
        "npm-run-all": "^4.1.5",
        "ts-essentials": "^4.0.0",
        "ts-loader": "^6.2.1",
        "typedoc": "^0.15.5",
        "typescript": "^3.7.3",
        "webpack": "^4.41.4",
        "webpack-bundle-analyzer": "^3.6.0",
        "webpack-cli": "^3.3.10"
    },
    "scripts": {
        "build:site": "npm-run-all build:site:typedoc build:demo build:site:demo build:site:docusaurus",
        "build:site:typedoc": "typedoc --out website/static/typedoc src --externalPattern index",
        "build:site:demo": "copyfiles demo/index.html demo/dist/* website/static",
        "build:site:docusaurus": "npm run build --prefix website",
        "build:site:docusaurus:start": "npm run start --prefix website",
        "build:demo": "webpack --config=demo/webpack.config.js",
        "build:demo:watch": "webpack --config=demo/webpack.config.js --watch",
        "build:demo:analyse": "webpack --config=demo/webpack.config.js --profile --json > dist/stats.json && webpack-bundle-analyzer dist/stats.json",
        "update": "npm-check-updates"
    }
}
